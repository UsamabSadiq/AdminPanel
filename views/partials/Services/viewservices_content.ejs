<div class="container-fluid px-4">
  <h1 class="mt-4">Services</h1>
  <ol class="breadcrumb mb-4">
    <!-- <li class="breadcrumb-item">
      <a href="/home">Dashboard</a>
    </li> -->
    <li class="breadcrumb-item active">Dashboard/Service/View Services</li>
  </ol>
  <table id="datatablesSimple">
    <thead>
      <tr>
        <th>Service Name</th>
        <th>Service Desc</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <% if(services.length> 0){ %> <% services.forEach(services=>{ %>
            <td>
              <%= services.ServiceName %>
            </td>
            <td>
              <%= services.ServiceDesc %>
            </td>
            <td>
              <a href="/viewservices_content/delete/<%= services._id %>" class="btn btn-danger btn-block">Delete</a>
              <a href="/viewservices_content/edit/<%= services._id %>" class="btn btn-primary btn-block">Edit</a>
            </td>
      </tr>
      <% }) %>
        <% } %>
    </tbody>
  </table>
</div>