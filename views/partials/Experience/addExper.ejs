<div class="container-fluid px-4">
    <h1 class="mt-4">Experience</h1>
    <ol class="breadcrumb mb-4">
        <!-- <li class="breadcrumb-item">
        <a href="/home">Dashboard</a>
      </li> -->
        <li class="breadcrumb-item active">Dashboard/Add Experience</li>
    </ol>

    <form method="post">
        <div class="row mb-3">
            <div class="col-md-6">
                <div class="form-floating mb-3 mb-md-0">
                    <input class="form-control" id="inputFirstName" type="text" name="OccuName"
                        placeholder="Enter your Occupation" />
                    <label for="inputFirstName">Occupation</label>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-floating">
                    <input class="form-control" id="inputCompName" name="CompName" type="text"
                        placeholder="Enter Company Name" />
                    <label for="inputLastName">Company Name</label>
                </div>
            </div>
            <br><br><br>
            <div class="col-md-6">
                <div class="form-floating">
                    <input class="form-control" id="inputSY" name="StartYear" type="date" placeholder="dd/mm/yy" />
                    <label for="inputDOI">Start Date</label>
                </div>
            </div>
            <br><br><br>

            <div class="col-md-6">
                <div class="form-floating">
                    <input class="form-control" id="inputED" name="EndYear" type="date" placeholder="dd/mm/yy" />
                    <label for="inputDOI">End Year</label>
                </div>
            </div>

            <div class="col-md-6">
                <div class="form-floating">
                    <input class="form-control" id="inputDesc" name="Desc" type="text" placeholder="Description" />
                    <label for="inputDOI">Description</label>
                </div>
            </div>

        </div>
        <div class="mt-4 mb-0">
            <div class="d-grid">
                <button class="btn btn-primary btn-block" href="/addservices" type="submit">
                    Submit
                </button>
            </div>
        </div>
    </form>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        crossorigin="anonymous"></script>
    <script src="js/scripts.js"></script>